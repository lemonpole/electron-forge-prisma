{
  "name": "electron-forge-prisma",
  "productName": "Electron Forge Prisma",
  "version": "1.0.0",
  "description": "A sample application built with Electron Forge and Prisma",
  "main": ".webpack/main",
  "author": "Steve Rivera",
  "license": "UNLICENSED",
  "homepage": "https://github.com/lemonpole/electron-forge-prisma",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lemonpole/electron-forge-prisma.git"
  },
  "bugs": {
    "url": "https://github.com/lemonpole/electron-forge-prisma/issues"
  },
  "keywords": [
    "prisma",
    "electron",
    "electron-forge",
    "forge"
  ],
  "scripts": {
    "docs": "npx typedoc --cleanOutputDir --entryPointStrategy expand src",
    "db:clean": "shx rm -rf node_modules/.prisma && shx rm -rf src/main/prisma/databases/save_0.db && shx rm -rf src/main/prisma/migrations/*_init",
    "db:init": "prisma migrate dev --schema src/main/prisma/schema.prisma --name init",
    "db:reset": "npm run db:clean && npm run db:init",
    "gen:icon": "magick src/renderer/assets/icon.png -background none -resize 256x256 src/renderer/assets/icon.ico",
    "lint": "eslint --ext .ts,.tsx .",
    "make": "electron-forge make",
    "package": "electron-forge package",
    "postinstall": "npm run db:init",
    "prepare": "husky install",
    "publish": "electron-forge publish",
    "start": "electron-forge start"
  },
  "prisma": {
    "seed": "ts-node -r tsconfig-paths/register src/main/prisma/seeds"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.6",
    "@commitlint/config-conventional": "17.6.6",
    "@electron-forge/cli": "6.2.1",
    "@electron-forge/maker-dmg": "6.2.1",
    "@electron-forge/maker-squirrel": "6.2.1",
    "@electron-forge/plugin-webpack": "6.2.1",
    "@electron-forge/publisher-github": "6.2.1",
    "@types/react": "18.0.20",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.60.1",
    "@typescript-eslint/parser": "5.60.1",
    "@vercel/webpack-asset-relocator-loader": "1.7.3",
    "css-loader": "6.7.1",
    "electron": "25.2.0",
    "electron-wix-msi": "5.0.0",
    "eslint": "8.44.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsdoc": "46.4.3",
    "fork-ts-checker-webpack-plugin": "7.2.13",
    "husky": "8.0.3",
    "node-loader": "2.0.0",
    "prettier": "2.8.8",
    "prisma": "4.9.0",
    "shx": "0.3.4",
    "style-loader": "3.3.1",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "4.9.4"
  },
  "dependencies": {
    "@chakra-ui/icons": "2.0.9",
    "@chakra-ui/react": "2.3.2",
    "@emotion/react": "11.10.4",
    "@emotion/styled": "11.10.4",
    "@prisma/client": "4.9.0",
    "countries-list": "2.6.1",
    "electron-is": "3.0.0",
    "electron-log": "4.4.8",
    "electron-squirrel-startup": "1.0.0",
    "flagpack": "1.0.5",
    "framer-motion": "7.3.5",
    "lodash": "4.17.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-select": "5.7.0",
    "react-tsparticles": "2.10.1",
    "sqlite3": "5.1.6",
    "tsparticles": "2.10.1",
    "tsparticles-engine": "2.10.1",
    "tsparticles-preset-snow": "2.10.1"
  }
}
